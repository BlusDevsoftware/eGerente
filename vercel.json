{
  "version": 2,
  "builds": [
    {
      "src": "frontend/src/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/", "dest": "/frontend/src/index.html" },
    { "src": "/usuarios", "dest": "/frontend/src/usuarios.html" },
    { "src": "/colaboradores", "dest": "/frontend/src/colaboradores.html" },
    { "src": "/produtos", "dest": "/frontend/src/produtos.html" },
    { "src": "/servicos", "dest": "/frontend/src/servicos.html" },
    { "src": "/clientes", "dest": "/frontend/src/clientes.html" },
    { "src": "/conferencia", "dest": "/frontend/src/conferencia.html" },
    { "src": "/consulta-comissao", "dest": "/frontend/src/consulta-comissao.html" },
    { "src": "/dinamico", "dest": "/frontend/src/dinamico.html" },
    { "src": "/lancar-comissao", "dest": "/frontend/src/lancar-comissao.html" },
    { "src": "/manutencao-bd", "dest": "/frontend/src/manutencao-bd.html" },
    { "src": "/movimento-comissao", "dest": "/frontend/src/movimento-comissao.html" },
    { "src": "/recebimento", "dest": "/frontend/src/recebimento.html" },
    { "src": "/comissoes", "dest": "/frontend/src/comissoes.html" },
    { "src": "/sincronizar", "dest": "/frontend/src/sincronizar.html" },
    { "src": "/login", "dest": "/frontend/src/login.html" },
    { "src": "/assets/(.*)", "dest": "/frontend/src/assets/$1" },
    { "src": "/styles/(.*)", "dest": "/frontend/src/styles/$1" },
    { "src": "/(.*)", "dest": "/frontend/src/$1" }
  ]
} 