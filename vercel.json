{
  "version": 2,
  "builds": [
    {
      "src": "frontend/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/js/(.*)",
      "dest": "/frontend/js/$1"
    },
    {
      "src": "/css/(.*)",
      "dest": "/frontend/css/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/frontend/assets/$1"
    },
    {
      "src": "/Paginas_JS/(.*)",
      "dest": "/frontend/Paginas_JS/$1"
    },
    {
      "src": "/Paginas_HTML/(.*)",
      "dest": "/frontend/Paginas_HTML/$1"
    },
    {
      "src": "/",
      "dest": "/frontend/Paginas_HTML/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ]
} 