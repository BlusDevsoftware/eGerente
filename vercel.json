{
  "version": 2,
  "builds": [
    {
      "src": "frontend/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/Paginas_JS/(.*)",
      "dest": "/Paginas_JS/$1"
    },
    {
      "src": "/Paginas_HTML/(.*)",
      "dest": "/Paginas_HTML/$1"
    },
    {
      "src": "/usuarios",
      "dest": "/Paginas_HTML/usuarios.html"
    },
    {
      "src": "/usuarios.html",
      "dest": "/Paginas_HTML/usuarios.html"
    },
    {
      "src": "/colaboradores",
      "dest": "/Paginas_HTML/colaboradores.html"
    },
    {
      "src": "/colaboradores.html",
      "dest": "/Paginas_HTML/colaboradores.html"
    },
    {
      "src": "/produtos",
      "dest": "/Paginas_HTML/produtos.html"
    },
    {
      "src": "/produtos.html",
      "dest": "/Paginas_HTML/produtos.html"
    },
    {
      "src": "/servicos",
      "dest": "/Paginas_HTML/servicos.html"
    },
    {
      "src": "/servicos.html",
      "dest": "/Paginas_HTML/servicos.html"
    },
    {
      "src": "/clientes",
      "dest": "/Paginas_HTML/clientes.html"
    },
    {
      "src": "/clientes.html",
      "dest": "/Paginas_HTML/clientes.html"
    },
    {
      "src": "/",
      "dest": "/Paginas_HTML/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 