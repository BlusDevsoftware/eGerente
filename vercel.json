{
  "version": 2,
  "builds": [
    {
      "src": "frontend/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/Paginas_HTML/index.html"
    },
    {
      "src": "/usuarios",
      "dest": "/Paginas_HTML/usuarios.html"
    },
    {
      "src": "/usuarios.html",
      "dest": "/Paginas_HTML/usuarios.html"
    },
    {
      "src": "/colaboradores",
      "dest": "/Paginas_HTML/colaboradores.html"
    },
    {
      "src": "/colaboradores.html",
      "dest": "/Paginas_HTML/colaboradores.html"
    },
    {
      "src": "/produtos",
      "dest": "/Paginas_HTML/produtos.html"
    },
    {
      "src": "/produtos.html",
      "dest": "/Paginas_HTML/produtos.html"
    },
    {
      "src": "/servicos",
      "dest": "/Paginas_HTML/servicos.html"
    },
    {
      "src": "/servicos.html",
      "dest": "/Paginas_HTML/servicos.html"
    },
    {
      "src": "/clientes",
      "dest": "/Paginas_HTML/clientes.html"
    },
    {
      "src": "/clientes.html",
      "dest": "/Paginas_HTML/clientes.html"
    },
    {
      "src": "/perfis",
      "dest": "/Paginas_HTML/perfis.html"
    },
    {
      "src": "/perfis.html",
      "dest": "/Paginas_HTML/perfis.html"
    },
    {
      "src": "/parametros",
      "dest": "/Paginas_HTML/parametros.html"
    },
    {
      "src": "/parametros.html",
      "dest": "/Paginas_HTML/parametros.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 