{
  "version": 2,
  "builds": [
    {
      "src": "frontend/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/js/(.*)",
      "dest": "/frontend/js/$1"
    },
    {
      "src": "/css/(.*)",
      "dest": "/frontend/css/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/frontend/assets/$1"
    },
    {
      "src": "/Paginas_JS/(.*)",
      "dest": "/frontend/Paginas_JS/$1"
    },
    {
      "src": "/usuarios",
      "dest": "/frontend/Paginas_HTML/usuarios.html"
    },
    {
      "src": "/colaboradores",
      "dest": "/frontend/Paginas_HTML/colaboradores.html"
    },
    {
      "src": "/produtos",
      "dest": "/frontend/Paginas_HTML/produtos.html"
    },
    {
      "src": "/servicos",
      "dest": "/frontend/Paginas_HTML/servicos.html"
    },
    {
      "src": "/clientes",
      "dest": "/frontend/Paginas_HTML/clientes.html"
    },
    {
      "src": "/",
      "dest": "/frontend/Paginas_HTML/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ]
} 