{
    "version": 2,
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/cadastros/(.*)",
            "dest": "src/index.js"
        },
        {
            "src": "/health",
            "dest": "src/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "src/index.js"
        }
    ]
} 