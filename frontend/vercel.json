{
    "version": 2,
    "builds": [
        {
            "src": "**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/js/(.*)",
            "dest": "/frontend/js/$1"
        },
        {
            "src": "/Paginas_JS/(.*)",
            "dest": "/frontend/Paginas_JS/$1"
        },
        {
            "src": "/Paginas_CSS/(.*)",
            "dest": "/frontend/Paginas_CSS/$1"
        },
        {
            "src": "/Imagens/(.*)",
            "dest": "/frontend/Imagens/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/frontend/$1"
        }
    ]
} 